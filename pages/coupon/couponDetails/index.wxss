/* pages/coupon/couponDetails/index.wxss */

page {
    width: 100%;
    height: 100%;
}

.mtop30 {
    margin-top: 30rpx;
}

.containner {
    width: 100%;
    height: 100%;
    background: #f5f5f5;
    font-size: 28rpx;
    color: #323232;
}

.coupon, .explain {
    background: #fff;
    padding: 25rpx 30rpx;
}

.titleWarp, .explain .title {
    padding-bottom: 25rpx;
    justify-content: space-between;
    border-bottom: 1px solid #dcdcdc;
}

.type {
    color: #63afe0;
}

.content {
    padding-top: 20rpx;
}

.item {
    line-height: 50rpx;
}

.price{
    color: #DA0808;
}
.hasAddress {
    justify-content: space-between;
}

.go {
    align-items: center;
    color: #63afe0;
}

.go image {
    width: 20rpx;
    height: 30rpx;
    margin-left: 20rpx;
}

.buy {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background: rgba(99, 224, 140, 1);
    text-align: center;
    line-height: 100rpx;
    font-size: 36rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
}
