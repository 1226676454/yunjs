page{
    width: 100%;
    height: 100%;
}

.containner{
    width: 100%;
    min-height: 100%;
    background: #F5F5F5;
    font-size: 32rpx;
    padding-top: 30rpx;
    color: #323232;
    box-sizing: border-box;
}

image{
    width: 44rpx;
    height: 44rpx;
}

.item{
    padding: 25rpx 30rpx;
    background: #fff;
    margin-bottom: 30rpx;
    justify-content: space-between;
}

.iphone{
    margin-right: 70rpx;
}
.action image{
    margin-left: 30rpx;
}

.add{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background: rgba(99, 224, 140, 1);
    text-align: center;
    line-height: 100rpx;
    font-size: 36rpx;
    font-family: PingFang-SC-Bold;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
}