/* pages/webviwe/index.wxss */
page{
    width: 100%;
    height: 100%;
}

.warp{
    width: 100%;
    min-height: 100%;
    box-sizing: border-box;
    padding-bottom: 100rpx;
    position: relative;
}
image{
    width: 100%;
    display:block;
}

.bottomwarp{
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    
}

.news {
    font-size: 24rpx;
    color: #63AFE0;
    align-items: center;
    justify-content: center;
    flex-basis: 200rpx;
    background: #fff;
}
.news image{
    width: 48rpx;
    height: 48rpx;
    margin-bottom: 5rpx;
}

.confirm{
    width: 100%;
    background: #63E08C;
    color: #fff;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
}
